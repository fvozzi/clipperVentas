SET SCOREBOARD ON
SET BELL ON
SET CONFIRM OFF
SET DELETE ON
SET CENTURY ON
SET DATE BRITISH
SET EXCLUSIVE OFF
RESTORE FROM FISCAL.MEM
STORE 83 TO JJ
DO WHILE JJ = 83
SET COLOR TO W/N
RESTORE SCREEN FROM PANT1
SET COLOR TO W+/RB
@ 2,6 SAY "                     CIERRE DE IMPRESOR FISCAL"
SET COLOR TO W/B
@ 8,14 CLEAR TO 8,69
@ 7,14,9,69 BOX &caja9
@ 8,18 SAY "Fecha(dd/mm/aaaa):"
STORE DATE() TO RFECHA
@ 8,37 GET RFECHA
READ
STORE 3 TO VMENU
SET COLOR TO W+/RB
@ 23,22 PROMPT "CIERRE Z"
@ 23,52 PROMPT "FINALIZAR"
MENU TO VMENU
IF VMENU = 1
ERASE CIERREZ.OUT
RUN CIERREZ /C:2 
EXIT
ENDIF
IF VMENU = 2
EXIT
ENDIF
ENDDO
CLOSE ALL
RETURN
